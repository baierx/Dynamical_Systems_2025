import plotly.graph_objects as go
import numpy as np

def plot_3d_volume(data, title="3D Volume Visualization", 
                   colorscale='RdBu_r', opacity=0.1, frame_duration=100):
    """
    Create volumetric rendering with cloud-like appearance and animation controls.
    
    Parameters:
        data: 4D numpy array of shape (samples, n, n, n)
        title: plot title
        colorscale: plotly colorscale name
        opacity: transparency (0-1)
        frame_duration: milliseconds per frame in animation
    """
    samples, n, _, _ = data.shape
    
    vmin = -2
    vmax =  2
    
    fig = go.Figure()
    
    for sample_idx in range(samples):
        fig.add_trace(go.Volume(
            x=np.arange(n).repeat(n*n),
            y=np.tile(np.arange(n).repeat(n), n),
            z=np.tile(np.arange(n), n*n),
            value=data[sample_idx].flatten(),
            isomin=vmin,
            isomax=vmax,
            opacity=opacity,
            surface_count=21,  # More surfaces = smoother clouds
            colorscale=colorscale,
            visible=(sample_idx == 0),
            showscale=True,
            colorbar=dict(title="Value", thickness=15, len=0.7)
        ))
    
    # Create slider steps
    steps = []
    for i in range(samples):
        step = dict(
            method="update",
            args=[{"visible": [False] * samples},
                  {"title": f"{title} - Sample {i}"}],
            label=str(i)
        )
        step["args"][0]["visible"][i] = True
        steps.append(step)
    
    # Create slider with play/pause buttons
    sliders = [dict(
        active=0,
        yanchor="top",
        y=-0.1,
        xanchor="left",
        currentvalue=dict(
            prefix="Sample: ",
            visible=True,
            xanchor="right"
        ),
        pad=dict(b=10, t=50),
        len=0.9,
        x=0.05,
        steps=steps,
        transition=dict(duration=frame_duration),
    )]
    
    # Add play and pause buttons
    updatemenus = [dict(
        type="buttons",
        direction="left",
        x=0.05,
        y=-0.25,
        xanchor="left",
        yanchor="top",
        pad=dict(r=10, t=10),
        showactive=True,
        buttons=[
            dict(
                label="▶ Play",
                method="animate",
                args=[None, {
                    "frame": {"duration": frame_duration, "redraw": True},
                    "fromcurrent": True,
                    "transition": {"duration": frame_duration, "easing": "linear"},
                    "mode": "immediate"
                }]
            ),
            dict(
                label="⏸ Pause",
                method="animate",
                args=[[None], {
                    "frame": {"duration": 0, "redraw": False},
                    "mode": "immediate",
                    "transition": {"duration": 0}
                }]
            )
        ]
    )]
    
    # Create frames for animation
    frames = []
    for i in range(samples):
        frame_data = [False] * samples
        frame_data[i] = True
        frames.append(go.Frame(
            data=[go.Volume(visible=v) for v in frame_data],
            name=str(i),
            layout=go.Layout(title_text=f"{title} - Sample {i}")
        ))
    
    fig.frames = frames
    
    fig.update_layout(
        title=f"{title} - Sample 0",
        scene=dict(
            xaxis=dict(title='X'),
            yaxis=dict(title='Y'),
            zaxis=dict(title='Z'),
            aspectmode='cube'
        ),
        sliders=sliders,
        updatemenus=updatemenus,
        height=600,
        width =600
    )
    
    return fig

# Usage:
fig = plot_3d_volume(data_norm[::30, :, :, :], opacity=0.15, colorscale='RdBu_r', frame_duration=200)
    
