from scipy.integrate   import solve_ivp
from matplotlib.pyplot import subplots


def model(t, variables):
    """Harmonic Oscillator"""
    
    X, Y = variables
    
    dXdt = - Y
    dYdt = + X
    
    return [dXdt, dYdt]


initial_conditions = [1, 2]

time_span = (0, 100)

solution = solve_ivp(model, t_span, initial_conditions)

t = solution.t

S = solution.y.T

fig, ax = subplots(figsize=(5, 3))




